{
  "name": "colabot-vscode",
  "displayName": "ColaBOT: AI assistant",
  "description": "Extension that uses AI to help you code faster",
  "version": "1.3.0",
  "engines": {
    "vscode": "^1.75.0"
  },
  "publisher": "ColaBOT",
  "icon": "assets/icon.png",
  "categories": [
    "Data Science",
    "Programming Languages",
    "Snippets",
    "Testing",
    "Machine Learning",
    "Education",
    "Other"
  ],
  "author": {
    "name": "Felipe Ossandon",
    "email": "fe.ossandon.u@gmail.com"
  },
  "repository": {
    "url": "https://github.com/felipetodev/colabot"
  },
  "main": "./out/extension.js",
  "activationEvents": [],
  "contributes": {
    "menus": {
      "editor/context": [
        {
          "command": "colabot-vscode.askCode",
          "group": "colabot_commands"
        },
        {
          "command": "colabot-vscode.explainCode",
          "group": "colabot_commands"
        }
      ]
    },
    "keybindings": [
      {
        "command": "colabot-vscode.getCode",
        "key": "ctrl+shift+i",
        "mac": "cmd+shift+i",
        "when": "editorTextFocus"
      }
    ],
    "commands": [
      {
        "command": "colabot-vscode.getCode",
        "title": "Get Code",
        "category": "ColaBOT"
      },
      {
        "command": "colabot-vscode.setApiKey",
        "title": "Set API Key",
        "category": "ColaBOT"
      },
      {
        "command": "colabot-vscode.removeApiKey",
        "title": "Remove API Key",
        "category": "ColaBOT"
      },
      {
        "command": "colabot-vscode.askCode",
        "title": "Ask ColaBOT",
        "category": "ColaBOT"
      },
      {
        "command": "colabot-vscode.explainCode",
        "title": "Explain ColaBOT",
        "category": "ColaBOT"
      },
      {
        "command": "colabot-vscode.aiCommit",
        "title": "AI Commit",
        "category": "ColaBOT (Git)"
      }
    ],
    "configuration": {
      "title": "ColaBOT ðŸ¤–",
      "type": "object",
      "properties": {
        "colaBot.apiKey": {
          "type": "string",
          "description": "To use ColaBOT press (ctrl+shift+p) and search for 'ColaBOT: Set API Key'. then select your AI provider:",
          "default": "openai",
          "enum": [
            "openai",
            "cohere"
          ],
          "order": 1
        },
        "colaBot.model": {
          "type": "string",
          "description": "The model to use for ColaBOT",
          "default": "gpt-3.5-turbo",
          "enum": [
            "gpt-3.5-turbo",
            "text-davinci-003",
            "command-xlarge-nightly",
            "summarize-xlarge"
          ],
          "order": 2
        },
        "colaBot.maxTokens": {
          "type": "number",
          "description": "The maximum number of tokens to generate",
          "default": 1500,
          "order": 3
        },
        "colaBot.temperature": {
          "type": "number",
          "description": "The temperature of the model (between 0 and 1)",
          "default": 0.3,
          "order": 4
        },
        "colaBot.gitMoji": {
          "type": "boolean",
          "description": "Use gitmoji in your SemVer commits",
          "default": true,
          "order": 5
        },
        "colaBot.semanticVersioningSpecification": {
          "type": "boolean",
          "description": "Use semantic versioning specification in your commits",
          "default": true,
          "order": 6
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "pnpm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "pnpm run compile && pnpm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.75.0",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "@vscode/test-electron": "^2.2.2",
    "eslint": "^8.33.0",
    "glob": "^8.1.0",
    "mocha": "^10.1.0",
    "prettier": "^2.8.4",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "cohere-ai": "^6.0.0",
    "eventsource-parser": "^0.1.0",
    "openai": "^3.2.1"
  }
}
